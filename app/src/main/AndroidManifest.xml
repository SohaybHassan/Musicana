<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.prography.musicana">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.MusicaApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".feature.regester.view.VerificationCode">

        </activity>
        <activity
            android:name=".feature.channelsdata.ChannelsDataActivity"
            android:parentActivityName=".feature.channels.ChannelsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".feature.channels.ChannelsActivity" />
        </activity>
        <activity
            android:name=".feature.channels.ChannelsActivity"
            android:parentActivityName=".feature.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".feature.MainActivity" />
        </activity>
        <activity android:name=".feature.bottomNavigationViewFragment.home.search.SearchActivity" />
        <activity
            android:name=".feature.forgotPassword.ForgotPasswordActivity"
            android:parentActivityName=".feature.login.view.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".feature.login.view.LoginActivity" />


        </activity>
        <activity android:name=".feature.regester.view.RegesterActivity" />
        <activity android:name=".feature.login.view.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity
            android:name=".feature.onboard.view.PrivacyPolicyActivity"
            android:parentActivityName=".feature.onboard.view.Onboarding">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".feature.onboard.view.Onboarding" />
        </activity>
        <activity
            android:name=".feature.onboard.view.TermsConditionsActivity"
            android:parentActivityName=".feature.onboard.view.Onboarding">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".feature.onboard.view.Onboarding" />
        </activity>
        <activity android:name=".feature.onboard.view.Onboarding" />
        <activity android:name=".feature.MainActivity"></activity>
        <activity android:name=".feature.SplashActivity">


        </activity>

        <service android:name=".feature.bottomNavigationViewFragment.home.phoneFragment.model.MusicService">
            <intent-filter>
                <action android:name="android.media.browse.MediaBrowserService" />
            </intent-filter>
        </service>
    </application>

</manifest>